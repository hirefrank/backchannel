{
  "name": "network-intel",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run build:client && bun run dev:server",
    "dev:server": "bun --watch server/src/index.ts",
    "dev:client": "cd client && bun vite",
    "dev:dual": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "db:migrate": "bun server/src/db/migrate.ts",
    "build": "bun run build:client",
    "build:client": "cd client && bun vite build",
    "start": "bun run build && bun server/src/index.ts",
    "test": "bun test server/src"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.20",
    "ai": "^6.0.68",
    "concurrently": "^9.0.1"
  }
}
